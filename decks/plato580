asm.
user(plato,plato)
settl(*)
setasl(*)
setjsl(*)
rfl,170000.
attach,procfil.
attach,opl=plaopl.
attach,nos=opl580.
attach,ipl.
attach,cstc.
attach,pcommon/m=w,na.
if(.not.file(pcommon,as))define,pcommon.
*---
* master
*
modify(z,lo=e)/*oplfile nos,ipl/*edit lowcor
cstc(l=0,a,i,s=nostext,ml=580d)
modify(z,lo=e)/*oplfile nos,ipl/*edit instalp
compass(l=0,a,i,s=nostext,ml=580d)
modify(z,lo=e)/*oplfile nos,ipl/*edit datime/*edit msubs/*edit psys
cstc(l=0,i,c=pcommon,s=nostext,ml=580d)
modify(lo=e)
cstc(l=0,i,c=pcommon,s=nostext,ml=580d)
*---
*         tutor
*
modify(lo=e)
cstc(l=0,i,c=pcommon,s=nostext,ml=580d)
*---
*         overlays
*
modify(lo=e)
cstc(l=0,i,c=pcommon,s=nostext,ml=580d)
*---
*         genfig
*
purge(genfig/na)
define(genfig/ct=pu,m=r)
modify(z,lo=e)/*oplfile nos/*edit genfigx
compass(l=0,i,d,b=genfig)
unload,genfig.
*
rewind(lgo)
purge(plato/na)
define(plato/ct=pu,m=r)
copybf(lgo,plato)
unload(plato)
*---
attach(genfig)
attach(plato)
unload(lgo,compile)
genfig,plato.
compass(i,s=0,l=0)
unload,genfig.
*---
begin(loader,,rel=$plato,lgo$,abs=pbin,rfl=0,map=pmap)
rfl,60000.
note(dir)+*i abs/plato,ovl/*
unload,lgo.
gtr(pbin,lgo)ovl/plat00$
libedit(p=pbin,c,i=dir,lo=e)
begin(ulib,,pbin,ulibp,plato)
catalog(pbin,r,n,u)
unload(pbin,plato,pmap)
