job(cm72000,t77770,p17)
user(plato,plato)
attach(opl=wpl/un=wpl)
attach(ptext)
attach(mtext)
attach(iptext)
attach(miscdef)
attach(syscon)
attach(ecsprty)
attach(ecstext)
*
purge,mrqbin/na.
define,mrqbin.
*
attach(mrq)
attach(mas)
attach(mxx)
attach(spj)
attach(mfn)
*
compass(i=mrq,s=nostext,b=mrqbin)
exit.
exit.
compass(i=mas,s=nostext,b=mrqbin)
compass(i=mxx,s=nostext,b=mrqbin)
compass(i=spj,s=nostext,b=mrqbin)
compass(i=mfn,s=nostext,b=mrqbin)
