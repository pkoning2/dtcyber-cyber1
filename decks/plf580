asm.
user(plato,plato)
settl(*)
setasl(*)
setjsl(*)
rfl,170000.
attach,opl=plaopl.
attach,nos=opl580.
attach,ipl.
attach,cstc.
attach,lgo=plfbin/m=w,na.
if(.not.file(lgo,as))define,lgo=plfbin.
attach,plfcom/m=w,na.
if(.not.file(plfcom,as))define,plfcom.
*
*         plfcom.
*
modify(z,lo=e)/*oplfile nos,ipl/*edit plfcom
cstc(a,i,s=0,g=nostxt,ml=580d)
*
*         main  ident.
*
modify(z,lo=e)/*oplfile nos,ipl/*edit plfmain/*edit plfsubs
cstc(d,i,c=plfcom,s=0,g=nostxt,ml=580d)
*
*         other main overlay idents.
*
modify(lo=e)
cstc(d,i,c=plfcom,s=0,g=nostxt,ml=580d)
*
*         overlays.
*
modify(lo=e)
cstc(d,i,c=plfcom,s=0,g=nostxt,ml=580d)
*
*
exit.
exit.
* *** need to figure out this loader/ulib stuff.
purge(plf/na)
define(plf/ct=pu,m=r)
rewind(lgo)
copyei(lgo,plf)
catalog(plf,r,u,n)
*
begin(loader,,rel=plf,abs=plfbin,rfl=30000,mo=sbex,map=plfmap)
rewind(plfmap)
copyei(plfmap)
*
rfl(60000)
begin(ulib,,plfbin,ulibplf,plf)
catalog(plfbin,r,u,n)
*
~
*oplfile nos,ipl
*edit     plfbdd
*edit     plfbds
*edit     plfncr
*edit     plfoeb
~
